
import java.util.*;
public class main
{
	public static void main(String[] args) {
	    System.out.println("Enter your number");
		scanner sc= new Scanner(System.in);
		int a = sc.nextInt();
		if(a>=10 && a<=99)
		System.out.println("It is a 2 digit number");
		else
		System.out.println("It is a mot a 2 digit no");
	}
}
